10 REM PC-BASIC test 
20 REM DEFtype syntax
30 OPEN "ERRORS.TXT" FOR OUTPUT AS 1
40 ON ERROR GOTO 1000
50 DEFINT
60 DEFINT "1"
70 DEFINT A$
80 DEFINT A
90 DEFINT B-
100 DEFINT B-D
110 DEFINT -Z
120 DEFINT A-B, D-F, G-H: I%=0: H=1: SWAP H, I%
130 DEFSNG A-B, D-F, G-H: I!=0: H=1: SWAP H, I!
140 DEFDBL A-B, D-F, G-H: I#=0: H=1: SWAP H, I#
150 DEFSTR A-B, D-F, G-H: I$="": H="1": SWAP H, I$
999 END
1000 PRINT#1, ERR, ERL
1010 RESUME NEXT
