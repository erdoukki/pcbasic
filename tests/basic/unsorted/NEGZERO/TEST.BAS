10 OPEN "OUTPUT.TXT" FOR OUTPUT AS 1
15 ON ERROR GOTO 1000
20 Z%=0: N%=-Z%: PRINT#1, MKI$(Z%), MKI$(N%), Z%=N%, SGN(Z%), SGN(N%)
30 Z!=0: N!=-Z!: PRINT#1, MKS$(Z!), MKS$(N!), Z!=N!, SGN(Z!), SGN(N!)
40 Z#=0: N#=-Z#: PRINT#1, MKD$(Z#), MKD$(N#), Z#=N#, SGN(Z#), SGN(N#)
998 CLOSE
999 END
1000 PRINT#1, "error:", ERR, ERL
1010 RESUME NEXT

